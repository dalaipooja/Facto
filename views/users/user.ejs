  <main id="main">
    <!-- ======= Breadcrumbs ======= -->
    <div class="breadcrumbs" data-aos="fade-in">
      <div class="container">
        <h2>New Vacancies</h2>
        <p>Empower minds, shape futures. Join us in the journey of academic excellence and innovation. Together, let's inspire the next generation of leaders, thinkers, and change-makers </p>
      </div>
    </div><!-- End Breadcrumbs -->


    <!-- ======= Trainers Section ======= -->
    <section id="trainers" class="trainers">
      <div class="container" data-aos="fade-up">

        <div class="row" data-aos="zoom-in" data-aos-delay="100">
          <%
          if(vacancy.length > 0){
           for(let i=0;i<vacancy.length;i++){
             %>
          <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
            <div class="member">
              <img src="../images/t1.png" class="img-fluid" style="max-width: 30%;" alt="">
              <div class="member-content">
                <h4><%= vacancy[i].vacancyname %></h4>
                <span>Qualification: <%= vacancy[i].qualification %></span>
                <span>Stream: <%= vacancy[i].stream %></span>
                <span>Experience : <%= vacancy[i].experience %></span>
          
              </div>
            </div>
          </div>
   
      <%
    }
 }
 else{
    %>
    <tr>
        <td colspan="5">No Vacancy Available</td>
    </tr>
    <%
 }
 %>
</div>
</div>
 <div class="loginkro">
  <p id="new" style="font-weight: bolder;">Intersted! Want to apply for the vacany.<br></p>
  <a href="/logsign" class="logi" >Login/Signup</a>


</div>

    </section><!-- End Trainers Section -->

